{
  "name": "typed-graphqlify",
  "version": "2.4.0",
  "description": "Build Typed GraphQL Queries in TypeScript. A better TypeScript + GraphQL experience.",
  "keywords": [
    "graphql",
    "typescript",
    "utility"
  ],
  "repository": "git@github.com:acro5piano/typed-graphqlify",
  "author": "gosho-kazuya <ketsume0211@gmail.com>",
  "license": "MIT",
  "private": false,
  "main": "dist/index.js",
  "jsnext:main": "dist/index.es.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "clean": "rm -rf dist/*",
    "dist": "npm-run-all clean build",
    "format": "prettier --write src/*.ts src/**/*.ts README.md",
    "lint": "tslint --fix src/*.ts src/**/*.ts",
    "test": "jest --maxWorkers=4",
    "test-d": "yarn run tsc && yarn run tsd",
    "test:coverage": "jest --maxWorkers=4 --coverage",
    "prepublish": "npm run dist"
  },
  "dependencies": {},
  "peerDependencies": {
    "typescript": "^3.5.1"
  },
  "devDependencies": {
    "@types/jest": "^23.3.10",
    "@types/node": "^10.12.18",
    "@types/webpack-env": "^1.13.6",
    "codecov": "^3.1.0",
    "husky": "^4.2.5",
    "jest": "^23.6.0",
    "lint-staged": "^10.1.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "rollup": "^2.21.0",
    "rollup-plugin-typescript2": "^0.24.3",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "tsd": "^0.11.0",
    "tslib": "^1.9.3",
    "tslint": "^6.1.2",
    "typescript": "^3.5.1"
  },
  "resolutions": {
    "handlebars": "4.5.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run format && git add ."
    }
  }
}
